packsArray = [];
init_new_pack();

function init_new_pack(){
	let _pack;
	<% @cards.each do |pack| %>
		_pack = [];
		<% pack.each do |card| %>
			_pack.push({
				name: '<%= card.name %>',
				cost: <%= card.cost %>,
				img_url: '<%= card.card_url %>'
			});
		<% end %>
		packsArray.push(_pack);
	<% end %>

	drawCards();

	console.log(packsArray);
}